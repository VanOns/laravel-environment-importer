{
    "name": "van-ons/laravel-environment-importer",
    "description": "A Laravel package for easy importing of a configured environment.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "environment",
        "import"
    ],
    "autoload": {
        "psr-4": {
            "VanOns\\LaravelEnvironmentImporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Danique Wijnalda",
            "email": "danique@van-ons.nl"
        },
        {
            "name": "Van Ons",
            "email": "opensource@van-ons.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": ">=10",
        "spatie/db-dumper": "^3.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "orchestra/testbench": "^7.0|^8.17|^9.0|^10.0",
        "phpstan/phpstan": "^1.12|^2.0"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VanOns\\LaravelEnvironmentImporter\\LaravelEnvironmentImporterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
